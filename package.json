{
  "name": "@bindings/helpers",
  "version": "1.0.0",
  "description": "Helper functions taken from a few places. Is it a small, not sure where to put. Put it up here!",
  "repository": {
    "type": "git",
    "url": "https://github.com/renoirb/experiments-201907-bili-babel-preset-env-multiple-target-builds.git"
  },
  "license": "UNLICENSED",
  "files": [
    "dist"
  ],
  "main": "dist",
  "module": "dist/index.esm.js",
  "scripts": {
    "build": "yarn run build:js && yarn run build:browser",
    "build:browser": "yarn run build:browser:cjs && yarn run build:browser:system",
    "build:browser:cjs": "bili --verbose --stack-trace --file-name index.browser.cjs.js --format cjs --target browser",
    "build:browser:system": "bili --verbose --stack-trace --file-name index.browser.system.js --format system --target browser",
    "build:js": "bili --verbose --stack-trace --target node",
    "preinstall": "node -e \"if(process.env.npm_execpath.indexOf('yarn') === -1) throw new Error('frontend must be installed with Yarn: https://yarnpkg.com/')\"",
    "lint": "prettier '**/*.{js,md}' --ignore-path .gitignore --write",
    "prepublishOnly": "yarn run build",
    "test": "jest --verbose --maxWorkers=3 --detectOpenHandles",
    "test:watch": "jest --verbose --watch --maxWorkers=3 --detectOpenHandles"
  },
  "devDependencies": {
    "@bindings/linting": "file:__elsewhere__/linting",
    "@bindings/packaging-and-testing": "file:__elsewhere__/packaging-and-testing",
    "lodash-es": "^4.17.10",
    "rollup-plugin-local": "file:__elsewhere__/rollup-plugin-local"
  },
  "engines": {
    "node": ">=10",
    "yarn": ">=1.5.0"
  },
  "copyright.owner": "ACME Corporation"
}
